{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "smartsport",
      "image": "samatlabibulla/smartsport:latest",
      "hostname": "smartsport",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8091
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "hostname": "redis",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    }
  ]
}